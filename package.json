{
  "name": "simpel-logger",
  "displayName": "Simpel-Logger",
  "description": "Bro, it's a simple logger for VSCodeüî•. It shows console logs inline in the editor without any BLOATWAREüò´. Just pure, simple loggingüôè.",
  "author": {
    "name": "Ren√© Oun"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reneoun/simpel-logger"
  },
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "inlineConsole.toggle",
        "title": "Toggle Inline Console Logs"
      },
      {
        "command": "inlineConsole.clear",
        "title": "Clear Inline Console Logs"
      },
      {
        "command": "inlineConsole.refresh",
        "title": "Refresh Inline Console Logs"
      }
    ],
    "configuration": {
      "title": "Simpel Logger",
      "properties": {
        "simpelLogger.executionTimeout": {
          "type": "number",
          "default": 3000,
          "description": "Execution timeout in milliseconds"
        },
        "simpelLogger.debounceDelay": {
          "type": "number",
          "default": 500,
          "description": "Debounce delay for updates in milliseconds"
        },
        "simpelLogger.truncateLength": {
          "type": "number",
          "default": 60,
          "description": "Maximum length of inline output before truncation"
        },
        "simpelLogger.enableOnStartup": {
          "type": "boolean",
          "default": false,
          "description": "Enable inline console logs when VS Code starts"
        }
      }
    },
    "keybindings": [
      {
        "command": "inlineConsole.toggle",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "@babel/generator": "^7.28.0",
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.1",
    "@types/http-proxy": "^1.17.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "http-proxy": "^1.18.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.7"
  }
}
